<Window x:Class="CNC.Controls.Camera.Camera"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CNC.Controls.Camera"
        mc:Ignorable="d"
        Title="Sender" Height="414" Width="504"
        Topmost="True"
        Closing="Window_Closing" Icon="App.ico" ShowInTaskbar="False" ResizeMode="NoResize">
    <Window.TaskbarItemInfo>
        <TaskbarItemInfo/>
    </Window.TaskbarItemInfo>
    <Grid>
        <local:CameraControl x:Name="CNCCamera" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="2,2"/>
    </Grid>
</Window>
